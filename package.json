{
  "name": "tiny-agency",
  "version": "1.0.1",
  "author": "Peter Bray <peter@foxandgeese.com>",
  "description": "Super-simple Material Design starter.",
  "scripts": {
    "develop": "gatsby develop",
    "lint": "eslint src/ && prettier --check 'src/**/*.js'",
    "fix": "eslint --fix src/ && prettier --write 'src/**/*.js'",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "deploy": "yarn && gatsby build --prefix-paths && gh-pages -d public"
  },
  "dependencies": {
    "@material-ui/core": "4.10.1",
    "core-js": "3.6.5",
    "gatsby": "2.22.20",
    "gatsby-plugin-favicon": "3.1.6",
    "gatsby-plugin-google-analytics": "2.3.3",
    "gatsby-plugin-manifest": "2.4.10",
    "gatsby-plugin-material-ui": "2.1.9",
    "gatsby-plugin-react-helmet": "3.3.3",
    "gatsby-plugin-remove-serviceworker": "1.0.0",
    "gatsby-source-filesystem": "2.3.10",
    "gatsby-transformer-remark": "2.8.14",
    "gh-pages": "3.0.0",
    "jss": "10.2.0",
    "mdi-material-ui": "6.16.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-jss": "10.2.0",
    "react-style-tag": "2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.1.0",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "prettier": "2.0.5"
  }
}
